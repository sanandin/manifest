  <project path="raspberrypi2_base_dir/meta-browser" remote="browser" name="OSSystems/meta-browser.git" revision="master" />
  <project path="raspberrypi2_base_dir/meta-ui" remote="browser" name="sanandin/meta-ui.git" revision="master" />
